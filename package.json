{
  "scripts": {
    "ts:w": "tsc -w",
    "ts": "tsc",
    "prestart": "rm -rf build && tsc",
    "start": "node build/index.js",
    "dev": "nodemon",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "verbose": true,
    "exec": "ts-node ./src/index.ts --project ./tsconfig.json",
    "execMap": {
      "ts": "ts-node"
    }
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.1",
    "@types/validator": "^13.1.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "module-alias": "^2.2.2",
    "mysql2": "^2.2.5",
    "sequelize": "^6.3.5"
  },
  "_moduleAliases": {
    "@utils": "build/Utils/index.js",
    "@api": "build/API",
    "@db": "build/DB"
  }
}
